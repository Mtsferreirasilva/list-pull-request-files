{
  "manifest_version": 2,
  "name": "List Pull Request Files",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": {
      "38": "icon.png"
    },
    "default_title": "List Pull Request Files",
    "default_popup": "index.html"
  },
  "author": ["Mateus Ferreira Silva"],
  "permissions": ["tabs", "<all_urls>"],
  "content_scripts": [ {
    "matches": ["https://github.com/*/files"],
    "css": ["styles.css"],
    "js": ["scrape.js", "clipboard.min.js", "main.js"]
  } ]
}
